[color]
	ui = true
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
	pager = "diff-so-fancy | less --tabs=4 -RFX"
[user]
	name = John Arthur
	email = john@jtarthur.net
[push]
	default = simple
[difftool "sourcetree"]
	cmd = /Applications/DeltaWalker.app/Contents/MacOS/Deltawalker \"$REMOTE\" \"$LOCAL\" -mi -pwd=\"`pwd`\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/DeltaWalker.app/Contents/MacOS/Deltawalker \"$REMOTE\" \"$LOCAL\" \"$BASE\"  -merged=\"$MERGED\" -mi -pwd=\"`pwd`\"
	trustExitCode = true
[credential]
	helper = osxkeychain
[commit]
	template = ~/.stCommitMsg
